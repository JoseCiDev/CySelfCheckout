{
    "compilerOptions": {
        "paths": {
            "@badeball/cypress-cucumber-preprocessor/*": [
                "./node_modules/@badeball/cypress-cucumber-preprocessor/dist/subpath-entrypoints/*"
            ]
        },
        "esModuleInterop": true,
        "moduleResolution": "Node",
        "target": "es5",
        "lib": [
            "es5",
            "dom"
        ],
        "types": [
            "cypress",
            "node"
        ]
    },
    "include": [
        "**/*.ts",
        "../cypress.d.ts"
    ]
}